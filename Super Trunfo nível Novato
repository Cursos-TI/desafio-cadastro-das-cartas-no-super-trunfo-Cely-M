#include <stdio.h>

int main() {
  
    char estado1 [50];
    char codigo1 [10];
    char cidade1 [40];
    int populacao1, pontosturisticos1;
    float area1, pib1;
     
    char estado2 [50];
    char codigo2 [10];
    char cidade2 [40];
    int populacao2, pontosturisticos2;
    float area2, pib2;

printf("Estado 1: \n");
scanf("%s", &estado1);
printf("Digite o codigo da carta: \n");
scanf("%s", &codigo1);

printf("Cidade: \n");
scanf("%s", &cidade1);

printf("População 1: \n");
scanf("%f", &populacao1);

printf("Pontos Turisticos 1: \n");
scanf("%f", &pontosturisticos1);

printf("Área 1: \n");
scanf("%f", &area1);

printf("PIB 1: \n");
scanf("%f", &pib1);

printf("Estado 2: \n");
scanf("%s", &estado2);
printf("Digite o codigo da carta 2: \n");
scanf("%s", &codigo2);

printf("Cidade 2: \n");
scanf("%s", &cidade2);

printf("População 2: \n");
scanf("%f", &populacao2);

printf("Pontos Turisticos 2: \n");
scanf("%f", &pontosturisticos2);

printf("Área 2: \n");
scanf("%f", &area2);

printf("PIB: \n");
scanf("%f", &pib2);

if (&populacao1>= &populacao2)
{
    printf ("Carta 1 venceu! \n)");
}
else {("Carta 2 Venceu! \n");
} 

return 0;

}
